{
"extends": "airbnb",
"env": {
    "browser": true,
    "node": true,
  },
  "rules": {
    "new-cap" : 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-boolean-value": 0,
    "react/forbid-prop-types": 0,
    "react/sort-comp": [1, {
      order: [
        'static-methods',
        'lifecycle',
        '/^init.+$/',
        'everything-else',
        'render'
      ]
    }]
  },
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
  "globals": {
      "globalSetting": false,
      "CKEDITOR": false,
    }
}
