{
"env": {
    "browser": true,
    "node": true,
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "new-cap" : 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-boolean-value": 0,
    "react/forbid-prop-types": 0,
  },
  "plugins": [
    "react"
  ],
  "globals": {
      "globalSetting": false,
      "CKEDITOR": false,
    }
}
